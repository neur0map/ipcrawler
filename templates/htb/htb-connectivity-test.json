{
  "name": "htb-connectivity-test",
  "tool": "curl",
  "args": ["-s", "--max-time", "10", "--connect-timeout", "5", "-I", "http://{{target}}"],
  "description": "HTB connectivity test - check if target web server is reachable before running other scans",
  "author": "HTB Team",
  "version": "1.0.0",
  "tags": ["htb", "connectivity", "test", "debug"],
  "dependencies": ["curl"],
  "timeout": 15,
  "target_types": ["ip", "domain", "url"],
  "severity": "low",
  "stealth": true,
  "parallel_safe": true
} 