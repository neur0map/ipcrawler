{
  "name": "htb-ffuf-subdomain-domain",
  "tool": "ffuf",
  "args": ["-u", "http://FUZZ.{{target}}", "-w", "{{wordlist}}", "-H", "User-Agent: Mozilla/5.0 HTB-Scanner", "-ac", "-t", "50", "-timeout", "10", "-mc", "200,204,301,302,307,401,403,405,500"],
  "description": "HTB FFUF subdomain discovery - USE WITH DISCOVERED DOMAIN, NOT IP (run htb-domain-extractor first)",
  "author": "HTB Team",
  "version": "1.0.0",
  "tags": ["htb", "subdomain-enum", "domain-required", "ffuf", "dns"],
  "dependencies": ["ffuf", "seclists"],
  "wordlist": "/usr/share/seclists/Discovery/DNS/subdomains-top1million-5000.txt",
  "timeout": 300,
  "target_types": ["domain"],
  "severity": "medium",
  "stealth": false,
  "parallel_safe": true
} 