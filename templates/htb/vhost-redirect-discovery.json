{
  "name": "vhost-redirect-discovery",
  "tool": "curl",
  "preset": "curl.vhost_check",
  "args": ["-L", "--max-redirs", "5", "-H", "Host: {{target}}", "http://{{target}}/", "-w", "Status: %{http_code}\\nRedirect: %{redirect_url}\\nFinal URL: %{url_effective}\\n"],
  "description": "Virtual host discovery through redirect analysis using curl",
  "tags": ["htb", "web", "vhost", "redirect", "enumeration"]
}