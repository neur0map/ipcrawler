{
  "name": "htb-ffuf-vhost",
  "tool": "ffuf",
  "args": ["-u", "http://{{target}}", "-H", "Host: FUZZ.{{target}}", "-w", "{{wordlist}}", "-H", "User-Agent: Mozilla/5.0 HTB-Scanner", "-ac", "-t", "50", "-timeout", "10", "-fc", "400,404"],
  "description": "HTB FFUF virtual host discovery with automatic calibration for wildcard filtering",
  "author": "HTB Team",
  "version": "1.0.0",
  "tags": ["htb", "vhost-enum", "virtual-host", "ffuf", "wildcard-detection"],
  "dependencies": ["ffuf", "seclists"],
  "wordlist": "/usr/share/seclists/Discovery/DNS/subdomains-top1million-5000.txt",
  "timeout": 300,
  "target_types": ["ip", "domain"],
  "severity": "medium",
  "stealth": false,
  "parallel_safe": true
} 