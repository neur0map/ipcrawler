{
  "name": "htb-nmap-full",
  "tool": "nmap",
  "args": ["-T5", "--min-rate", "5000", "--max-rate", "10000", "-p-", "-sS", "-sV", "-sC", "-O", "--traceroute", "--script", "vuln", "{{target}}"],
  "description": "HTB aggressive nmap scan with T5 timing, full port range, and comprehensive information gathering",
  "author": "HTB Team",
  "version": "1.0.0",
  "tags": ["htb", "nmap", "aggressive", "full-scan", "service-detection"],
  "dependencies": ["nmap"],
  "timeout": 300,
  "target_types": ["ip", "domain"],
  "severity": "high",
  "stealth": false,
  "parallel_safe": false
} 