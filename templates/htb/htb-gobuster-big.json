{
  "name": "htb-gobuster-big",
  "tool": "gobuster",
  "args": ["dir", "-u", "http://{{target}}", "-w", "{{wordlist}}", "-x", "php,html,txt,js,css,zip,bak,old,asp,aspx,jsp,json,xml", "-k", "-r", "-q", "--wildcard", "-H", "Host: {{target}}", "-H", "User-Agent: Mozilla/5.0 HTB-Scanner", "-t", "30"],
  "description": "HTB Gobuster thorough directory enumeration with SecLists big wordlist for comprehensive coverage",
  "author": "HTB Team",
  "version": "1.0.0",
  "tags": ["htb", "directory-enum", "web-discovery", "gobuster", "thorough", "big-wordlist"],
  "dependencies": ["gobuster", "seclists"],
  "wordlist": "/usr/share/seclists/Discovery/Web-Content/big.txt",
  "timeout": 300,
  "target_types": ["ip", "domain", "url"],
  "severity": "medium",
  "stealth": false,
  "parallel_safe": true
} 