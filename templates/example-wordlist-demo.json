{
  "name": "example-wordlist-demo",
  "tool": "gobuster",
  "args": ["dir", "-u", "{{target}}", "-w", "{{wordlist:fast}}", "-x", "php,html"],
  "description": "Demo template showing self-contained wordlist mapping",
  "author": "Demo",
  "version": "1.0.0",
  "tags": ["demo", "wordlist-example"],
  "dependencies": ["gobuster"],
  "wordlists": {
    "fast": "/usr/share/seclists/Discovery/Web-Content/common.txt",
    "thorough": "/usr/share/seclists/Discovery/Web-Content/big.txt",
    "custom": "/opt/my-custom-wordlist.txt"
  },
  "timeout": 60,
  "target_types": ["url", "domain"],
  "severity": "low",
  "parallel_safe": true
} 