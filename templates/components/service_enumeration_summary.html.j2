{% if service.enumeration_data %}
<div class="enum-section">
    <strong>üìä Enumeration:</strong>
    <ul class="styled-list">
        {% for key, value in service.enumeration_data.items() %}
        <li><strong>{{ key.replace('_', ' ').title() }}:</strong> {{ value | format_complex_value | safe }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.access_info %}
<div class="access-section">
    <strong>üîê Access Info:</strong>
    <ul class="styled-list">
        {% for key, value in service.access_info.items() %}
        <li><strong>{{ key.replace('_', ' ').title() }}:</strong> {{ value | format_complex_value | safe }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.config_info %}
<div class="config-section">
    <strong>‚öôÔ∏è Configuration:</strong>
    <ul class="styled-list">
        {% for key, value in service.config_info.items() %}
        {% if key == 'host_keys' %}
        <li><strong>SSH Host Keys:</strong> {{ value | format_ssh_keys | safe }}</li>
        {% elif key == 'shares' %}
        <li><strong>SMB Shares:</strong> {{ value | format_smb_shares | safe }}</li>
        {% elif key == 'users' %}
        <li><strong>Users:</strong> {{ value | format_user_list | safe }}</li>
        {% elif key == 'config' %}
        <li><strong>Configuration:</strong> {{ value | format_config_dict | safe }}</li>
        {% elif key == 'files' %}
        <li><strong>Files:</strong> {{ value | format_file_list | safe }}</li>
        {% elif key == 'databases' %}
        <li><strong>Databases:</strong> {{ value | format_user_list | safe }}</li>
        {% else %}
        <li><strong>{{ key.replace('_', ' ').title() }}:</strong> {{ value | format_complex_value | safe }}</li>
        {% endif %}
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.vulnerabilities %}
<div class="vuln-section">
    <strong>Vulnerabilities:</strong>
    <ul class="styled-list">
        {% for vuln in service.vulnerabilities %}
        <li class="vulnerability">{{ vuln }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if not (service.enumeration_data or service.access_info or service.config_info or service.vulnerabilities) %}
<span style="color: #888; font-style: italic;">No enumeration summary available</span>
{% endif %}