{% if service.enumeration_data %}
<div class="enum-section">
    <strong>Enumeration:</strong>
    <ul class="styled-list">
        {% for key, value in service.enumeration_data.items() %}
        <li>{{ key }}: {{ value }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.access_info %}
<div class="access-section">
    <strong>Access Info:</strong>
    <ul class="styled-list">
        {% for key, value in service.access_info.items() %}
        <li>{{ key }}: {{ value }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.config_info %}
<div class="config-section">
    <strong>Configuration:</strong>
    <ul class="styled-list">
        {% for key, value in service.config_info.items() %}
        <li>{{ key }}: {{ value }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if service.vulnerabilities %}
<div class="vuln-section">
    <strong>Vulnerabilities:</strong>
    <ul class="styled-list">
        {% for vuln in service.vulnerabilities %}
        <li class="vulnerability">{{ vuln }}</li>
        {% endfor %}
    </ul>
</div>
{% endif %}

{% if not (service.enumeration_data or service.access_info or service.config_info or service.vulnerabilities) %}
<span style="color: #888; font-style: italic;">No enumeration summary available</span>
{% endif %}