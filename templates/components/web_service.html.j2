<!-- Web Service Details -->
<div class="service-detail">
    <h4>
        <a href="{{ web_service.url }}" class="url" target="_blank">{{ web_service.url }}</a>
        {% if web_service.status_code %}
        <span style="color: {% if web_service.status_code < 400 %}#228b22{% else %}#dc143c{% endif %};">
            ({{ web_service.status_code }})
        </span>
        {% endif %}
    </h4>
    
    {% if web_service.title %}
    <p><strong>Title:</strong> {{ web_service.title }}</p>
    {% endif %}
    
    {% if web_service.server %}
    <p><strong>Server:</strong> {{ web_service.server }}</p>
    {% endif %}
    
    {% if web_service.technologies %}
    <div class="enum-section">
        <strong>Technologies:</strong>
        <ul class="styled-list">
            {% for tech in web_service.technologies %}
            <li>{{ tech }}</li>
            {% endfor %}
        </ul>
    </div>
    {% endif %}
    
    {% if web_service.directories %}
    <div class="enum-section">
        <strong>Discovered Directories:</strong>
        <ul class="styled-list">
            {% for dir in web_service.directories %}
            <li><a href="{{ web_service.url }}{{ dir }}" class="url" target="_blank">{{ dir }}</a></li>
            {% endfor %}
        </ul>
    </div>
    {% endif %}
    
    {% if web_service.files %}
    <div class="enum-section">
        <strong>Discovered Files:</strong>
        <ul class="styled-list">
            {% for file in web_service.files %}
            <li><a href="{{ web_service.url }}{{ file }}" class="url" target="_blank">{{ file }}</a></li>
            {% endfor %}
        </ul>
    </div>
    {% endif %}
    
    {% if web_service.vulnerabilities %}
    <div class="vuln-section">
        <strong>Vulnerabilities:</strong>
        <ul class="styled-list">
            {% for vuln in web_service.vulnerabilities %}
            <li class="vulnerability">{{ vuln }}</li>
            {% endfor %}
        </ul>
    </div>
    {% endif %}
</div>